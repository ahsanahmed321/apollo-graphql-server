{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "pretty": true,
    "outDir":"dist/",
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "tests/**/*"]
}
